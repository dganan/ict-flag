<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="edu.uoc.ictflag.ela" transaction-type="JTA">
		<jta-data-source>jdbc/ictflag</jta-data-source>
		<class>edu.uoc.ictflag.security.model.User</class>
		<class>edu.uoc.ictflag.security.model.UserSecurePassword</class>
 		<class>edu.uoc.ictflag.security.model.UserRoleConverter</class>
		<class>edu.uoc.ictflag.institution.model.Program</class>
		<class>edu.uoc.ictflag.institution.model.Subject</class>
 		<class>edu.uoc.ictflag.institution.model.Semester</class>
 		<class>edu.uoc.ictflag.institution.model.Course</class>
 		<class>edu.uoc.ictflag.institution.model.CourseGroup</class>
 		<class>edu.uoc.ictflag.institution.model.CourseGroupMember</class>
		<class>edu.uoc.ictflag.core.LocalizedString</class>
		<class>edu.uoc.ictflag.ela.model.Tool</class>
 		<class>edu.uoc.ictflag.ela.model.Exercise</class>
 		<class>edu.uoc.ictflag.ela.model.Assignment</class>
		<class>edu.uoc.ictflag.ela.model.EventData</class>
 		<class>edu.uoc.ictflag.ela.model.ExerciseData</class>
 		<class>edu.uoc.ictflag.ela.model.ExerciseDataRaw</class>
 		<class>edu.uoc.ictflag.ela.model.AssignmentData</class>
 		<class>edu.uoc.ictflag.ela.model.AssignmentDataRaw</class>
 		<class>edu.uoc.ictflag.ela.model.ActionConverter</class>
 		<class>edu.uoc.ictflag.ela.model.ElementTypeConverter</class>
 		<class>edu.uoc.ictflag.ela.model.ResultTypeConverter</class>
		<class>edu.uoc.ictflag.ela.model.OutcomeConverter</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<shared-cache-mode>NONE</shared-cache-mode>
	</persistence-unit>
</persistence>
